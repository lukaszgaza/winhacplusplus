<?xml version="1.0" encoding="UTF-8"?>

<settings xmlns="http://www.winhac.com"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.winhac.com UserFile.xsd">
	<hardProcess>
		<quarks>
			<name>u</name>
			<name>ubar</name>
			<name>d</name>
			<name>dbar</name>
			<name>c</name>
			<name>cbar</name>
			<name>s</name>
			<name>sbar</name>
			<name>b</name>
			<name>bbar</name>
			<name>t</name>
			<name>tbar</name>			
		</quarks>
		<bosons>
			<name>W+</name>
		</bosons>
		<leptons>
			<name>mu</name>
		</leptons>
		<polarizations>
			<polarization>unpolarized</polarization>
		</polarizations>
		<radiation>
			<level>Above + O(alpha) EW corrections in W-decay</level>
			<softPhotonCutOff>1.0E-3</softPhotonCutOff>
		</radiation>
		<others>
			<modelReferenceFrame>LAB</modelReferenceFrame>
			<factorizationScale>Invariant mass</factorizationScale>
			<alphaScheme>Alpha QED</alphaScheme>
			<widthScheme>fixed</widthScheme>
		</others>
	</hardProcess>
	<external>
		<pythiaXMLdocDir>/home/sobol/pakiety/pythia8153/xmldoc</pythiaXMLdocDir>
		<partonShower>false</partonShower>
		<hadronization>false</hadronization>
	</external>
	<weights>
		<weightedEvents>true</weightedEvents>
		<maxWeightRejection>2.0</maxWeightRejection>
		<main>
			<modelWeight>Born</modelWeight>
			<polarization>unpolarized</polarization>
		</main>
		<crude>
			<crudeWeight>XsCrudWt</crudeWeight>
			<crudeWeight>PartonsGeneration</crudeWeight>
			<crudeWeight>HardProcessWeight</crudeWeight>
		</crude>
		<model>
			<modelWeight>YFS O(alf0)</modelWeight>
			<modelWeight>beta01_QED</modelWeight>
			<modelWeight>beta11</modelWeight>
			<modelWeight>beta01_EW</modelWeight>
			<modelWeight>beta01_LL</modelWeight>
			<modelWeight>beta11_LL</modelWeight>
			<modelWeight>YFS O(alf1)_QED</modelWeight>
			<modelWeight>YFS O(alf1)_EW</modelWeight>
			<modelWeight>YFS O(alf1)_LL</modelWeight>
			<modelWeight>Born</modelWeight>
			<modelWeight>O(alpha)_QED</modelWeight>
			<modelWeight>O(alpha)_EW</modelWeight>
			<modelWeight>Born + virtual_QED + soft</modelWeight>
			<modelWeight>Born + virtual_EW + soft</modelWeight>
			<modelWeight>One real hard photon</modelWeight>
		</model>
	</weights>
	<technical>
		<printOutLevel>Basic</printOutLevel>
	</technical>
</settings>
